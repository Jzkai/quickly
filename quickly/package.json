{
  "name": "corejs",
  "version": "0.0.1",
  "private": true,
  "description": "The polyfills and core Node.js libraries for the Quickly ES6/Node.js emulation layer for QML",
  "author": "Michael Spencer <sonrisesoftware@gmail.com>",
  "license": "MPL-2.0",
  "keywords": [
    "qmlify",
    "quickly",
    "qml",
    "es6"
  ],
  "homepage": "https://github.com/iBeliever/quickly",
  "bugs": "https://github.com/iBeliever/quickly/issues",
  "repository": "iBeliever/quickly",
  "scripts": {
    "build": "qmlify -d build src",
    "test": "qmlify -d tests/build tests && qmltestrunner -input tests/build",
    "install-qml": "cp -r build/* src/qmldir `qmake -query QT_INSTALL_QML`/Quickly"
  },
  "dependencies": {
    "url": "^0.11.0"
  },
  "devDependencies": {
    "babel-eslint": "^5.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^2.4.0",
    "eslint-plugin-class-property": "0.0.3"
  }
}
